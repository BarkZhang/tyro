<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tyro.mapper.UserMapper">
 
	<select id="selectUserList" resultType="User">
		SELECT id as id,
		name as userName,
		email as email
		FROM user
	</select>
 
</mapper>